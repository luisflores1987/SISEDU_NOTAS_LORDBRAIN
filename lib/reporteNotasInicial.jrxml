	<subDataset name="NOTASPROMEDIO" uuid="fe0687dd-a24f-4771-9b29-2cf54544dccf">
		<parameter name="PIDALUMNO" class="java.lang.Integer"/>
		<parameter name="PIDBIMESTRE" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[SELECT
	[NIDALUMNO],
	[NOMBRE COMPLETO],
	[NIVEL],
	[GRADO],
	[AREA],
	[CURSO],
	[IDBIMESTRE],
	[1]		AS	[NOTA 1],
	[2]		AS	[NOTA 2],
		CAST(ROUND((CONVERT(FLOAT,[1])+CONVERT(FLOAT,[2]))/2,0,0) AS VARCHAR)	AS  [PROMEDIO],
	CASE
		WHEN ROUND((CONVERT(FLOAT,[1])+CONVERT(FLOAT,[2]))/2,0,0) > 0 AND ROUND((CONVERT(FLOAT,[1])+CONVERT(FLOAT,[2]))/2,0,0) < 11
		THEN 'C'
		WHEN ROUND((CONVERT(FLOAT,[1])+CONVERT(FLOAT,[2]))/2,0,0) > 10 AND ROUND((CONVERT(FLOAT,[1])+CONVERT(FLOAT,[2]))/2,0,0) < 13
		THEN 'B'
		WHEN ROUND((CONVERT(FLOAT,[1])+CONVERT(FLOAT,[2]))/2,0,0) > 12 AND ROUND((CONVERT(FLOAT,[1])+CONVERT(FLOAT,[2]))/2,0,0) < 21
		THEN 'A'
	END	AS [LIT.]
FROM
(
SELECT
	A.nId_Alumno		[NIDALUMNO],
	P.sNombreCompleto	[NOMBRE COMPLETO],
	N.sDescripcion		[NIVEL],
	G.sDescripcion		[GRADO],
	A2.sDescripcion		[AREA],
	C.sDescripcion		[CURSO],
	AC.sNota			[NOTAS],
	AC.nIdTipoNota		[TIPONOTA],
	AC.nIdBimestre		[IDBIMESTRE]
FROM
	dbo.Persona AS P
	INNER JOIN dbo.Alumno AS A ON A.nId_Persona = P.nId_Persona
	INNER JOIN dbo.Nivel AS N ON	N.nIdNivel = A.nIdNivel
	INNER JOIN dbo.Grado AS G ON G.nIdGrado = A.nIdGrado
	INNER JOIN dbo.Alumno_Curso AS AC ON AC.nId_Alumno = A.nId_Alumno
	INNER JOIN dbo.Curso AS C ON C.nIdCurso = AC.nIdCurso
	INNER JOIN dbo.Area AS A2 ON A2.nIdArea = C.nIdArea
) A
PIVOT
(
	MAX(NOTAS) FOR TIPONOTA IN ([1], [2])
) PVTNOTAS
WHERE
	[NIDALUMNO] = $P{PIDALUMNO}
	AND
	[IDBIMESTRE] = $P{PIDBIMESTRE}]]>
		</queryString>
