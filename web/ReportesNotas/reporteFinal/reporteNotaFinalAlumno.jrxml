	<subDataset name="PROMEDIOGLOBAL" uuid="35ec9e2b-1cb6-4f87-92ae-963649fbfeaa">
		<parameter name="idAlumno" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[29]]></defaultValueExpression>
		</parameter>
		<parameter name="idGrado" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[1]]></defaultValueExpression>
		</parameter>
		<parameter name="sSeccion" class="java.lang.String">
			<defaultValueExpression><![CDATA['A']]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
						B.PUNTAJE,
						B.PROMEDIO,
						'' AS CONDUCTA,
						B.PUESTO
					FROM
					(
					SELECT
						C.IDALUMNO,
						C.NOMBRE,
						ROW_NUMBER() OVER(ORDER BY SUM(ROUND(CONVERT(FLOAT, C.[NOTAS]),2,0)) DESC)	[PUESTO],
						SUM(C.[NOTAS]) [PUNTAJE],
						AVG(CAST(C.[NOTAS] AS FLOAT)) [PROMEDIO],
						C.SECCION
					FROM
					(
							SELECT
								IDALUMNO,
								IDAREA,
								CURSO,
								CAST(ROUND((CAST([1] AS FLOAT) + CAST([2] AS FLOAT)+ CAST([3] AS FLOAT) + CAST([4] AS FLOAT))/4,0,0) AS INT) [NOTAS],
								NOMBRE,
								SECCION
							FROM
							
							(
							SELECT
								A.IDAREA,
								A.CURSO,
								A.BIMESTRE,
								A.NOTAS,
								A.IDALUMNO,
								A.NOMBRE,
								A.SECCION
							FROM
								[dbo].[fnTablaPruebaPromedio]($P{idGrado}, $P{sSeccion}) A 
							) A
							PIVOT
							(
								MAX(A.NOTAS) FOR A.BIMESTRE IN ([1],[2],[3],[4])
							) AS PVTNOTAS
					) C
					GROUP BY
						C.NOMBRE,
						C.IDALUMNO,
						C.SECCION
					) B
	WHERE
		B.IDALUMNO = $P{idAlumno}
	ORDER BY
		B.NOMBRE]]>
		</queryString>
		<field name="PUNTAJE" class="java.lang.Integer"/>
		<field name="PROMEDIO" class="java.lang.Double"/>
		<field name="CONDUCTA" class="java.lang.String"/>
		<field name="PUESTO" class="java.lang.Long"/>
	</subDataset>
