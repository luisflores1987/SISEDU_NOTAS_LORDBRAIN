	<subDataset name="Comportamiento" uuid="35b3644a-982e-49d8-bd7d-10c1f3198b6b">
		<parameter name="IDALUMNO" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="IDBIMESTRE" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT
	DISTINCT
	A.nId_Alumno		[NIDALUMNO],
	P.sNombreCompleto	[NOMBRE COMPLETO],
	N.sDescripcion		[NIVEL],
	G.nIdGrado			[IDGRADO],
	G.sDescripcion		[GRADO],
	AC.nIdBimestre		[IDBIMESTRE],
	B.sDescripcion		[BIMESTRE],
	A.sSeccion			[SECCION],
	ISNULL(NC.Nota, '')		[NOTA CONDUCTA],
	ISNULL(NC.Sugerencias, '')	[SUGERENCIAS],
						ISNULL(OL.[sTardanzas],			 '') [sTardanzas],
						ISNULL(OL.[sInasJustificadas],	 '') [sInasJustificadas],
						ISNULL(OL.[sInasInjustificadas], '') [sInasInjustificadas],
						ISNULL(OL.[nIdParticipacionPPFF],'') [nIdParticipacionPPFF]
FROM
	dbo.Persona AS P
	INNER JOIN dbo.Alumno AS A ON A.nId_Persona = P.nId_Persona
	INNER JOIN dbo.Nivel AS N ON	N.nIdNivel = A.nIdNivel
	INNER JOIN dbo.Grado AS G ON G.nIdGrado = A.nIdGrado
	INNER JOIN dbo.Alumno_Curso AS AC ON AC.nId_Alumno = A.nId_Alumno
	INNER JOIN dbo.Bimestre AS B ON B.nIdBimestre = AC.nIdBimestre
	INNER JOIN NotaComportamiento AS NC ON NC.nIdAlumno = A.nId_Alumno
	LEFT JOIN Otros_Libreta	AS OL	ON	OL.nId_Alumno = A.nId_Alumno
WHERE
	AC.nId_Alumno = $P{IDALUMNO}
	AND
	B.nIdBimestre = 4
	AND
	NC.nIdBimestre = 4
	AND
	OL.nIdBimestre = 4]]>
		</queryString>
