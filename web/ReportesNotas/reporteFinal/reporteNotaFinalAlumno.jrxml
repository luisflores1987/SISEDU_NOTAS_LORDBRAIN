	<subDataset name="SituacionFinal" uuid="5f8421e6-292d-4bb3-a6af-0aab9619e928">
		<parameter name="idAlumno" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[88]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
	IDALUMNO,
	REPITE = CASE WHEN IDNIVEL = 2 THEN
					CASE WHEN SUM(SESTADO) = 2	THEN
						'1'
					ELSE
						'0'
					END
			     WHEN IDNIVEL = 3 THEN
					CASE WHEN SUM(SESTADO) > 3 THEN
						'1'
					ELSE
						'0'
					END
			 ELSE
				'0'
			 END,
	PROMOVIDO = CASE WHEN IDNIVEL = 2 OR IDNIVEL = 3 THEN
					CASE WHEN SUM(SESTADO) = 0 	THEN
						'1'
					ELSE
						'0'
					END
					WHEN IDNIVEL = 1 THEN
						'1'
				ELSE
					'0'
				END
FROM
	fnTablaPruebaFinalTotal ($P{idAlumno})
GROUP BY
	IDALUMNO, IDNIVEL]]>
		</queryString>
		<field name="IDALUMNO" class="java.lang.Integer"/>
		<field name="REPITE" class="java.lang.String"/>
		<field name="PROMOVIDO" class="java.lang.String"/>
	</subDataset>
	<parameter name="IDALUMNO" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[365]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
	DISTINCT
	[NIDALUMNO],
	[NOMBRE COMPLETO],
	CAST([IDNIVEL] AS VARCHAR(2)) [IDNIVEL],
	[NIVEL],
	[IDGRADO],
	[GRADO],
	[SECCION]
FROM
(
SELECT
	A.nId_Alumno		[NIDALUMNO],
	P.sNombreCompleto		[NOMBRE COMPLETO],
	N.nIdNivel			[idNivel],
	N.sDescripcion		[NIVEL],
	G.nIdGrado			[IDGRADO],
	G.sDescripcion		[GRADO],
	A2.nIDArea			[IDAREA],
	A2.sDescripcion		[AREA],
	C.sDescripcion		[CURSO],
	AC.sNota			[NOTAS],
	AC.nIdTipoNota		[TIPONOTA],
	A.sSeccion			[SECCION]
FROM
	dbo.Persona AS P
	INNER JOIN dbo.Alumno AS A ON A.nId_Persona = P.nId_Persona AND A.sEstado = 1 AND P.sEstado = 1
	INNER JOIN dbo.Nivel AS N ON	N.nIdNivel = A.nIdNivel
	INNER JOIN dbo.Grado AS G ON G.nIdGrado = A.nIdGrado
	INNER JOIN dbo.Alumno_Curso AS AC ON AC.nId_Alumno = A.nId_Alumno
	INNER JOIN dbo.Curso AS C ON C.nIdCurso = AC.nIdCurso
	INNER JOIN dbo.Area AS A2 ON A2.nIdArea = C.nIdArea AND A2.sEstado = 1
) A
PIVOT
(
	MAX(NOTAS) FOR TIPONOTA IN ([1], [2])
) PVTNOTAS

WHERE
	[NIDALUMNO] = $P{IDALUMNO}
ORDER BY
	[NOMBRE COMPLETO]]]>
	</queryString>
