	<parameter name="IDBIMESTRE" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="IDGRADO" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
	DISTINCT
	P.sNombreCompleto,
	A.nId_Alumno		[NIDALUMNO]
FROM
	dbo.Persona P
	INNER JOIN	 dbo.Alumno AS A ON A.nId_Persona = P.nId_Persona
	INNER JOIN dbo.Grado AS G ON G.nIdGrado = A.nIdGrado
	INNER JOIN dbo.Alumno_Curso AS AC ON AC.nId_Alumno = A.nId_Alumno
	INNER JOIN dbo.Bimestre AS B ON B.nIdBimestre = AC.nIdBimestre

WHERE
	AC.nIdBimestre = $P{IDBIMESTRE}
	AND
	G.nIdGrado	= $P{IDGRADO}
ORDER BY
	P.sNombreCompleto]]>
	</queryString>
	<field name="sNombreCompleto" class="java.lang.String"/>
	<field name="NIDALUMNO" class="java.lang.Integer"/>
